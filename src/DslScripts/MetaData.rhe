Module Common
{
	DataStructure StructureMetadata
	{
		LongString Value;
	}
	
	Function GetStructureMetadata Common.StructureMetadata '(parameters, repository, userInfo) => 
	{
		return new Common.StructureMetadata
		{
			Value = _metadataProvider.GetStructureMetadata()
		};
	}'
	{
		RepositoryUses '_metadataProvider' 'Rhetos.FloydExtensions.MetadataProvider, Rhetos.FloydExtensions';
	}
	
}